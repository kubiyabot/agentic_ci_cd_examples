{
  "name": "flaky-test-demo",
  "version": "1.0.0",
  "description": "Demo project to showcase TeamCity's intelligent flaky test detection vs Jenkins",
  "scripts": {
    "test": "jest --runInBand --reporters=default --reporters=jest-teamcity --forceExit",
    "test:ci": "jest --ci --coverage --forceExit",
    "test:junit": "jest --ci --reporters=default --reporters=jest-junit --forceExit",
    "test:teamcity": "jest --runInBand --reporters=default --reporters=jest-teamcity --forceExit"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "axios": "^1.7.9",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-teamcity": "^1.12.0"
  }
}
