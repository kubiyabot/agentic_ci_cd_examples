{
  "name": "incident-learning-pipeline",
  "version": "1.0.0",
  "description": "CI/CD Pipeline Failure Learning with Kubiya Cognitive Memory",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest __tests__/unit/",
    "test:integration": "jest __tests__/integration/",
    "test:all": "jest --runInBand",
    "test:json": "jest --json --outputFile=test-results.json",
    "start": "node src/index.js",
    "lint": "eslint src/ __tests__/"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 10000,
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.js"
    ]
  }
}
